<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
         <menuContribution
           locationURI="popup:org.eclipse.ui.popup.any?after=additions">
        <command
              commandId="org.eclipse.stem.ui.command.compile"
              disabledIcon="icons/full/actions16/disabledRun.gif"
              icon="icons/full/actions16/Compile.gif"
              label="%_UI_STEM_Command_Compile"
              mnemonic="%_UI_STEM_Command_CompileMne"
              style="push"
              tooltip="%_UI_STEM_Command_CompileTT">
           <visibleWhen
                 checkEnabled="false">
              <reference
                    definitionId="org.eclipse.stem.ui.isexecutable">
              </reference>
           </visibleWhen>
        </command>
        </menuContribution>
   </extension>
   
   <extension
        point="org.eclipse.ui.commands">
     <category
           description="%_UI_STEM_Compile_Command_Cat_Desc"
           id="org.eclipse.stem.ui.compile"
           name="%_UI_STEM_Compile_Command_Cat">
     </category>
     <command
           categoryId="org.eclipse.stem.ui.compile"
           description="%_UI_STEM_Command_Compile_Desc"
           id="org.eclipse.stem.ui.command.compile"
           name="%_UI_STEM_Command_Compile">
     </command>
   </extension>
   <extension
        point="org.eclipse.ui.handlers">
     <handler
           class="org.eclipse.stem.ui.handlers.CompileCommandHandler"
           commandId="org.eclipse.stem.ui.command.compile">
        <!-- <activeWhen>
           <reference
                 definitionId="org.eclipse.stem.ui.executableNotRunning">
           </reference>
        </activeWhen>
        -->
     </handler>
   </extension>
</plugin>
