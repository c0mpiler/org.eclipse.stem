<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.stem.distributed.feature"
      label="%featureName"
      version="3.0.0.qualifier"
      provider-name="%providerName">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://download.eclipse.org/stem/update-site/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.stem.core"/>
      <import plugin="org.eclipse.stem.jobs"/>
      <import plugin="org.eclipse.stem.definitions"/>
      <import plugin="org.eclipse.stem.populationmodels"/>
      <import plugin="org.eclipse.stem.diseasemodels"/>
      <import plugin="org.eclipse.stem.data.geography"/>
   </requires>

   <plugin
         id="org.eclipse.stem.graphsynchronizer"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
